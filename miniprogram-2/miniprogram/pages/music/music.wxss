/* pages/music/music.wxss */
/* 下面是swiper也就是music页面中顶部的滑动面板css */
.swiper{
        width: 100%;
        height: 300rpx;
}
.image{
        width: 100%;
        height: 100%;
}

/* playList自定义组件的布局 */
.playList{
        display: flex;
        justify-content: space-around;
        flex-wrap: wrap;
        margin-top:10rpx ;
}
.title{
        margin-top:10rpx ;
        margin-left:20rpx ;
}
/* 排行榜的css */
.ranking{
       padding: 20rpx;
        height: 450rpx;
}
.ranking-image{
        width: 150rpx;
        height: 150rpx;
}
 .rankingtext .ranking-number{
        width: 100rpx;
        height: 100rpx;
        text-align: center;
        line-height: 100rpx;
        padding: 20rpx;
}
.rankingtext .ranking-name{
        height: 100rpx;
        line-height: 100rpx;
}
