/* components/playList/playList.wxss */
.playList{
       width: 220.rpx;
       /* 定位 */
       position: relative;
       /* 容器的内边距 */
       padding-bottom: 20rpx;
}
.playList-img{
        width: 100%;
        height: 220rpx;
        border-radius: 10rpx;
}
.playList-count{
        font-size:24rpx;
        color: #fff;
        text-shadow: 1px 0 0 rgba(0,0,0,0);
        position: absolute;
        right: 10rpx;
        top: 4rpx;
        padding-left: 26rpx;
          background: url(data:image/png;base64,) no-repeat 0 8rpx/22rpx 20rpx;

}
.playList-name{
        font-size: 26rpx;
        line-height: 1.2;
        padding: 4rpx 0 0 12rpx;
        display: -webkit-box;
        -webkit-box-orient:  vertical;
        -webkit-line-clamp: 2;
        overflow: hidden;
        text-overflow: ellipsis;
}
